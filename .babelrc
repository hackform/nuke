{
  "presets": [
    ["env", {
      "modules": false,
      "loose": true,
    }],
  ],
  "plugins": [
    ["transform-react-jsx", { pragma: "h" }],
    "syntax-dynamic-import",
  ],
  "env": {
    "server": {
      "presets": [
        ["env", {
          "targets": {
            "node": "current",
          },
        }],
      ],
      "plugins": [
        "transform-import-to-require",
      ],
    },
  },
}
